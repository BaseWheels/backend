{
  "name": "minigarage-backend",
  "version": "1.0.0",
  "description": "MiniGarage - Your personal digital garage. Collect legendary racing machines as NFTs on Base blockchain.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "postinstall": "prisma generate",
    "migrate:deploy": "prisma migrate deploy",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "web3",
    "nft",
    "blockchain",
    "base",
    "gacha",
    "gaming",
    "collectibles",
    "cars"
  ],
  "author": "MiniGarage Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@privy-io/server-auth": "^1.32.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.16.0",
    "express": "^4.21.2",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
